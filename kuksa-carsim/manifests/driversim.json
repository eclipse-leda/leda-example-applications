{
    "container_id": "driversimservice-example",
    "container_name": "driversimservice-example",
    "image": {
        "name": "ghcr.io/eclipse-leda/leda-example-driversim:main"
    },
    "host_config": {
        "devices": [],
        "network_mode": "bridge",
        "privileged": false,
        "restart_policy": {
            "maximum_retry_count": 0,
            "retry_timeout": 0,
            "type": "unless-stopped"
        },
        "runtime": "io.containerd.runc.v2",
        "extra_hosts": [
            "databroker:container_databroker-host"
        ],
        "port_mappings": [],
        "log_config": {
            "driver_config": {
                "type": "json-file",
                "max_files": 2,
                "max_size": "1M",
                "root_dir": ""
            },
            "mode_config": {
                "mode": "blocking",
                "max_buffer_size": ""
            }
        }
    },
    "config": {
        "env": [
           "DATABROKER_ADDRESS=databroker:55555",
           "LOG_LEVEL=INFO",
           "SIM_SPEED=2"
        ],
        "cmd": []
    }
}
